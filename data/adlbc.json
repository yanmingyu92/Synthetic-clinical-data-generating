{
    "datasets": [
      {
        "name": "ADLBC",
        "description": "Analysis Dataset Lab",
        "class": "BDS",
        "structure": "one record per subject per parameter per analysis visit",
        "keys": ["USUBJID", "PARAMCD", "AVISIT", "LBSEQ"],
        "location": "adlbc.xpt",
        "variables": [
          {"name": "STUDYID", "label": "Study Identifier", "type": "text", "length": 12, "source": "ADSL.STUDYID"},
          {"name": "SUBJID", "label": "Subject Identifier for the Study", "type": "text", "length": 4, "source": "ADSL.SUBJID"},
          {"name": "USUBJID", "label": "Unique Subject Identifier", "type": "text", "length": 11, "source": "ADSL.USUBJID"},
          {"name": "TRTP", "label": "Planned Treatment", "type": "text", "length": 20, "source": "ADSL.TRT01P"},
          {"name": "TRTPN", "label": "Planned Treatment (N)", "type": "integer", "length": 8, "source": "ADSL.TRT01PN"},
          {"name": "TRTA", "label": "Actual Treatment", "type": "text", "length": 20, "source": "ADSL.TRT01A"},
          {"name": "TRTAN", "label": "Actual Treatment (N)", "type": "integer", "length": 8, "source": "ADSL.TRT01AN"},
          {"name": "TRTSDT", "label": "Date of First Exposure to Treatment", "type": "integer", "length": 8, "source": "ADSL.TRTSDT"},
          {"name": "TRTEDT", "label": "Date of Last Exposure to Treatment", "type": "integer", "length": 8, "source": "ADSL.TRTEDT"},
          {"name": "AGE", "label": "Age", "type": "integer", "length": 8, "source": "ADSL.AGE"},
          {"name": "AGEGR1", "label": "Pooled Age Group 1", "type": "text", "length": 5, "source": "ADSL.AGEGR1"},
          {"name": "AGEGR1N", "label": "Pooled Age Group 1 (N)", "type": "integer", "length": 8, "source": "ADSL.AGEGR1N"},
          {"name": "RACE", "label": "Race", "type": "text", "length": 32, "source": "ADSL.RACE"},
          {"name": "RACEN", "label": "Race (N)", "type": "integer", "length": 8, "source": "ADSL.RACEN"},
          {"name": "SEX", "label": "Sex", "type": "text", "length": 1, "source": "ADSL.SEX"},
          {"name": "COMP24FL", "label": "Completers of Week 24 Population Flag", "type": "text", "length": 1, "source": "ADSL.COMP24FL"},
          {"name": "DSRAEFL", "label": "Discontinued due to AE?", "type": "text", "length": 1, "source": "ADSL.DSRAEFL"},
          {"name": "SAFFL", "label": "Safety Population Flag", "type": "text", "length": 1, "source": "ADSL.SAFFL"},
          {"name": "AVISIT", "label": "Analysis Visit", "type": "text", "length": 16, "source": "Analysis Visit"},
          {"name": "AVISITN", "label": "Analysis Visit (N)", "type": "integer", "length": 8, "source": "Numeric code for AVISIT"},
          {"name": "ADY", "label": "Analysis Relative Day", "type": "integer", "length": 8, "source": "LB.LBDY"},
          {"name": "ADT", "label": "Analysis Date", "type": "integer", "length": 8, "source": "LB.LBDTC"},
          {"name": "VISIT", "label": "Visit Name", "type": "text", "length": 19, "source": "LB.VISIT"},
          {"name": "VISITNUM", "label": "Visit Number", "type": "float", "length": 8, "source": "LB.VISITNUM"},
          {
            "name": "PARAM",
            "label": "Parameter",
            "type": "text",
            "length": 100,
            "source": "PARAM_ADLB",
            "code_list": [
              {"code": "ALB", "decode": "Albumin (g/L)"},
              {"code": "ALP", "decode": "Alkaline Phosphatase (U/L)"},
              {"code": "ALT", "decode": "Alanine Aminotransferase (U/L)"},
              {"code": "AST", "decode": "Aspartate Aminotransferase (U/L)"},
              {"code": "BILI", "decode": "Bilirubin (umol/L)"},
              {"code": "BUN", "decode": "Blood Urea Nitrogen (mmol/L)"},
              {"code": "CA", "decode": "Calcium (mmol/L)"},
              {"code": "CHOL", "decode": "Cholesterol (mmol/L)"},
              {"code": "CK", "decode": "Creatine Kinase (U/L)"},
              {"code": "CL", "decode": "Chloride (mmol/L)"},
              {"code": "CREAT", "decode": "Creatinine (umol/L)"},
              {"code": "GGT", "decode": "Gamma Glutamyl Transferase (U/L)"},
              {"code": "GLUC", "decode": "Glucose (mmol/L)"},
              {"code": "K", "decode": "Potassium (mmol/L)"},
              {"code": "PHOS", "decode": "Phosphate (mmol/L)"},
              {"code": "PROT", "decode": "Protein (g/L)"},
              {"code": "SODIUM", "decode": "Sodium (mmol/L)"},
              {"code": "URATE", "decode": "Urate (umol/L)"}
            ]
          },
          {
            "name": "PARAMCD",
            "label": "Parameter Code",
            "type": "text",
            "length": 8,
            "source": "PARAMCD_ADLB",
            "code_list": [
              {"code": "ALB", "decode": "Albumin"},
              {"code": "ALP", "decode": "Alkaline Phosphatase"},
              {"code": "ALT", "decode": "Alanine Aminotransferase"},
              {"code": "AST", "decode": "Aspartate Aminotransferase"},
              {"code": "BILI", "decode": "Bilirubin"},
              {"code": "BUN", "decode": "Blood Urea Nitrogen"},
              {"code": "CA", "decode": "Calcium"},
              {"code": "CHOL", "decode": "Cholesterol"},
              {"code": "CK", "decode": "Creatine Kinase"},
              {"code": "CL", "decode": "Chloride"},
              {"code": "CREAT", "decode": "Creatinine"},
              {"code": "GGT", "decode": "Gamma Glutamyl Transferase"},
              {"code": "GLUC", "decode": "Glucose"},
              {"code": "K", "decode": "Potassium"},
              {"code": "PHOS", "decode": "Phosphate"},
              {"code": "PROT", "decode": "Protein"},
              {"code": "SODIUM", "decode": "Sodium"},
              {"code": "URATE", "decode": "Urate"}
            ]
          },
          {"name": "PARAMN", "label": "Parameter (N)", "type": "integer", "length": 8, "source": "PARAMN_ADLB"},
          {"name": "PARCAT1", "label": "Parameter Category 1", "type": "text", "length": 5, "source": "ADLBCAT"},
          {"name": "AVAL", "label": "Analysis Value", "type": "float", "length": 8, "source": "Analysis Value"},
          {"name": "BASE", "label": "Baseline Value", "type": "float", "length": 8, "source": "Baseline Value"},
          {"name": "CHG", "label": "Change from Baseline", "type": "float", "length": 8, "source": "Change from Baseline"},
          {"name": "A1LO", "label": "Analysis Range 1 Lower Limit", "type": "float", "length": 8, "source": "LB.LBSTNRLO"},
          {"name": "A1HI", "label": "Analysis Range 1 Upper Limit", "type": "float", "length": 8, "source": "LB.LBSTNRHI"},
          {"name": "R2A1LO", "label": "Ratio to Analysis Range 1 Lower Limit", "type": "float", "length": 8, "source": "R2A1LO"},
          {"name": "R2A1HI", "label": "Ratio to Analysis Range 1 Upper Limit", "type": "float", "length": 8, "source": "R2A1HI"},
          {"name": "BR2A1LO", "label": "Base Ratio to Analysis Range 1 Lower Lim", "type": "float", "length": 8, "source": "BR2A1LO"},
          {"name": "BR2A1HI", "label": "Base Ratio to Analysis Range 1 Upper Lim", "type": "float", "length": 8, "source": "BR2A1HI"},
          {"name": "ANL01FL", "label": "Analysis Record Flag 1", "type": "text", "length": 1, "source": "Y_BLANK"},
          {"name": "ALBTRVAL", "label": "Amount Threshold Range", "type": "float", "length": 8, "source": "ALBTRVAL"},
          {"name": "ANRIND", "label": "Analysis Reference Range Indicator", "type": "text", "length": 1, "source": "LNH"},
          {"name": "BNRIND", "label": "Baseline Reference Range Indicator", "type": "text", "length": 1, "source": "LNH"},
          {"name": "ABLFL", "label": "Baseline Record Flag", "type": "text", "length": 1, "source": "Y_BLANK"},
          {"name": "AENTMTFL", "label": "Last value in treatment visit", "type": "text", "length": 1, "source": "Y_BLANK"},
          {"name": "LBSEQ", "label": "Sequence Number", "type": "integer", "length": 8, "source": "LB.LBSEQ"},
          {"name": "LBNRIND", "label": "Reference Range Indicator", "type": "text", "length": 8, "source": "LB.LBNRIND"},
          {"name": "LBSTRESN", "label": "Numeric Result/Finding in Standard Units", "type": "float", "length": 8, "source": "LB.LBSTRESN"}
        ]
      }
    ]
  }
  